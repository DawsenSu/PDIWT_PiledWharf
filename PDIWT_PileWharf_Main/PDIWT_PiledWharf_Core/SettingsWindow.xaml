    xmlns:mvvmLightCommand="http://www.galasoft.ch/mvvmlight"
    xmlns:valid="clr-namespace:PDIWT.Resources.ValidationRules;assembly=PDIWT.Resources"
    Title="{x:Static loc:Resources.Settings}"
    Width="300"
    Height="370"
