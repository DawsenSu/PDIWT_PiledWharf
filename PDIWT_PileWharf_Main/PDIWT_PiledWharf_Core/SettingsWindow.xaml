            <TabItem Header="{x:Static loc:Resources.General}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Label
                        Grid.Row="0"
                        Grid.Column="0"
                        Content="{x:Static loc:Resources.StructureImportanceFactor}" />
                    <TextBox Grid.Row="0" Grid.Column="1">
                        <Binding NotifyOnValidationError="True" Path="StructureImportanceFactor">
                            <Binding.ValidationRules>
                                <StaticResource ResourceKey="RealRangeValidationRule" />
                            </Binding.ValidationRules>
                        </Binding>
                    </TextBox>
                    <Label
                        Grid.Row="1"
                        Grid.Column="0"
                        Content="{x:Static loc:Resources.AntiEarthquakeIntensity}" />
                    <TextBox Grid.Row="1" Grid.Column="1">
                        <Binding NotifyOnValidationError="True" Path="AntiEarthquakeIntensity">
                            <Binding.ValidationRules>
                                <StaticResource ResourceKey="RealRangeValidationRule" />
                            </Binding.ValidationRules>
                        </Binding>
                    </TextBox>
                    <Label
                        Grid.Row="2"
                        Grid.Column="0"
                        Content="{x:Static loc:Resources.FixedDepthFactor}" />
                    <TextBox Grid.Row="2" Grid.Column="1">
                        <Binding NotifyOnValidationError="True" Path="FixedDepthFactor">
                            <Binding.ValidationRules>
                                <StaticResource ResourceKey="RealRangeValidationRule" />
                            </Binding.ValidationRules>
                        </Binding>
                    </TextBox>
                    <Label
                        Grid.Row="3"
                        Grid.Column="0"
                        Content="{x:Static loc:Resources.AntiEarthquakeAdjustmentFactor}" />
                    <TextBox Grid.Row="3" Grid.Column="1">
                        <Binding NotifyOnValidationError="True" Path="AntiEarthquakeAdjustmentFactor">
                            <Binding.ValidationRules>
                                <StaticResource ResourceKey="RealRangeValidationRule" />
                            </Binding.ValidationRules>
                        </Binding>
                    </TextBox>
                </Grid>
            </TabItem>
            <TabItem Header="{x:Static loc:Resources.Material}" TabIndex="3">
                <StackPanel>
                    <GroupBox Header="{x:Static loc:Resources.Concrete}">
                        <Grid>
