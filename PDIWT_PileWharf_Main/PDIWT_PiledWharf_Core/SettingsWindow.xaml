                            <Label
                                Grid.Row="3"
                                Grid.Column="0"
                                Content="{x:Static loc:Resources.UnderWaterUnitWeight}" />
                            <TextBox
                                Grid.Row="3"
                                Grid.Column="1"
                                TabIndex="7">
                                <Binding NotifyOnValidationError="True" Path="ConcreteUnderWaterUnitWeight">
                                    <Binding.ValidationRules>
                                        <StaticResource ResourceKey="RealRangeValidationRule" />
                                    </Binding.ValidationRules>
                                </Binding>
                            </TextBox>
                            <Label
                                Grid.Row="3"
                                Grid.Column="2"
                                Content="kN/m3" />
                        </Grid>
                    </GroupBox>
