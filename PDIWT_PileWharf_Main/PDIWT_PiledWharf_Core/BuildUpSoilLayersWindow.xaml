                                    </Setter>
                                </Style>
                            </DataGridTemplateColumn.HeaderStyle>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <UniformGrid Background="Transparent" Columns="2">
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding DPSE_Qfi, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,0,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding DPSE_Qr, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <UniformGrid Columns="2">
                                        <TextBox Text="{Binding DPSE_Qfi, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding DPSE_Qr, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Width="210">
                            <DataGridTemplateColumn.HeaderStyle>
                                <Style TargetType="DataGridColumnHeader">
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="DataGridColumnHeader">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="1" />
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="1" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
