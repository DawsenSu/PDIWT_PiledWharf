                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISAGP_Betasi, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISAGP_Psisi, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
