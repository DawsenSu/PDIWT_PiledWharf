                                        <TextBox Text="{Binding CISAGP_Betap, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding CISAGP_Psip, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding CISAGP_Qr, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
