                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISAGP_Betap, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
