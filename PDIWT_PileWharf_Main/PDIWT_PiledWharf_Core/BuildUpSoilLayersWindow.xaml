                                                    <Border
                                                        Grid.Row="2"
                                                        Grid.Column="9"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.RowSpan="4"
                                                        Grid.Column="11"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Label
                                                        Grid.Row="0"
                                                        Grid.ColumnSpan="11"
                                                        HorizontalContentAlignment="Center"
                                                        Content="{x:Static loc:Resources.BCPileType_CastInSituAfterGrountingPile}" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="0"
                                                        HorizontalContentAlignment="Center"
                                                        Content="βsi" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="2"
                                                        HorizontalContentAlignment="Center"
                                                        Content="ψsi" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="4"
                                                        HorizontalContentAlignment="Center"
                                                        Content="qfi" />
                                                    <Label
