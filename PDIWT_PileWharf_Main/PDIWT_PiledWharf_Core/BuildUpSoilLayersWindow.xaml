                            </DataGridTemplateColumn.HeaderStyle>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <UniformGrid Background="Transparent" Columns="3">
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding TPSP_Qfi, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding TPSP_Yita, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,0,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding TPSP_Qr, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <UniformGrid Columns="3">
                                        <TextBox Text="{Binding TPSP_Qfi, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding TPSP_Yita, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding TPSP_Qr, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Width="280">
                            <DataGridTemplateColumn.HeaderStyle>
                                <Style TargetType="DataGridColumnHeader">
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="DataGridColumnHeader">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="1" />
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="1" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border
                                                        Grid.Row="1"
                                                        Grid.ColumnSpan="9"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="3"
                                                        Grid.ColumnSpan="9"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="2"
                                                        Grid.Column="1"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="2"
                                                        Grid.Column="3"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="2"
                                                        Grid.Column="5"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.RowSpan="4"
                                                        Grid.Column="7"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Label
                                                        Grid.Row="0"
                                                        Grid.ColumnSpan="7"
                                                        HorizontalContentAlignment="Center"
                                                        Content="{x:Static loc:Resources.BCPileType_CastInSituPile}" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="0"
                                                        HorizontalContentAlignment="Center"
                                                        Content="ψsi" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="2"
                                                        HorizontalContentAlignment="Center"
                                                        Content="qfi" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="4"
                                                        HorizontalContentAlignment="Center"
                                                        Content="ψp" />
                                                    <Label
                                                        Grid.Row="2"
                                                        Grid.Column="6"
                                                        HorizontalContentAlignment="Center"
                                                        Content="qR" />
                                                </Grid>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </DataGridTemplateColumn.HeaderStyle>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <UniformGrid Background="Transparent" Columns="4">
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISP_Psisi, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISP_Qfi, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,1,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISP_Psip, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                        <Border
                                            Padding="0"
                                            BorderBrush="{DynamicResource DataGridBorder}"
                                            BorderThickness="0,0,0,0">
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding CISP_Qr, UpdateSourceTrigger=PropertyChanged}" />
                                        </Border>
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <UniformGrid Columns="4">
                                        <TextBox Text="{Binding CISP_Psisi, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding CISP_Qfi, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding CISP_Psip, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding CISP_Qr, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellEditingTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Width="420">
                            <DataGridTemplateColumn.HeaderStyle>
                                <Style TargetType="DataGridColumnHeader">
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="DataGridColumnHeader">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="1" />
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="1" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="1" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border
                                                        Grid.Row="1"
                                                        Grid.ColumnSpan="11"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="3"
                                                        Grid.ColumnSpan="11"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="2"
                                                        Grid.Column="1"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="2"
                                                        Grid.Column="3"
                                                        Background="{DynamicResource DataGridBorder}" />
                                                    <Border
                                                        Grid.Row="2"
