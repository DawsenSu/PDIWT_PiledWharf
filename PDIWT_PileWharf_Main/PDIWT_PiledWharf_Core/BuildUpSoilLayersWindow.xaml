                                        </Border>
                                    </UniformGrid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                            <DataGridTemplateColumn.CellEditingTemplate>
                                <DataTemplate>
                                    <UniformGrid Columns="6">
                                        <TextBox Text="{Binding CISAGP_Betasi, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
                                        <TextBox Text="{Binding CISAGP_Psisi, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource nulldoulbe2str}}" />
