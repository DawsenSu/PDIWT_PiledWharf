                Style="{StaticResource BasicButton}" />
            <Button
                Grid.Column="2"
                Command="{Binding Confirm}"
                Content="{x:Static loc:Resources.OK}"
                IsDefault="True"
                Style="{StaticResource BasicButton}" />
            <Button
                Grid.Column="3"
                Command="{Binding Cancel}"
                Content="{x:Static loc:Resources.Cancel}"
                IsCancel="True"
                Style="{StaticResource BasicButton}" />
