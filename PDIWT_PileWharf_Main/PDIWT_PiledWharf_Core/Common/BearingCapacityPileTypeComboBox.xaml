<UserControl
    x:Class="PDIWT.Resources.CustomUserControl.BearingCapacityPileTypeComboBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PDIWT.Resources.CustomUserControl"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <ComboBox
        x:Name="BearingCapacityPileType_ComboBox"
        DisplayMemberPath="Value"
        SelectedValue="{Binding BearingCapacityPileType, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=local:BearingCapacityPileTypeComboBox}}"
        SelectedValuePath="Key" />
</UserControl>
